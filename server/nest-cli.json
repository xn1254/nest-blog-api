{
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/server/src",
  "monorepo": true,
  "root": "apps/server",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/server/tsconfig.app.json"
  },
  "projects": {
    "server": {
      "type": "application",
      "root": "apps/server",
      "entryFile": "main",
      "sourceRoot": "apps/server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/server/tsconfig.app.json"
      }
    },
    "web-server": {
      "type": "application",
      "root": "apps/web-server",
      "entryFile": "main",
      "sourceRoot": "apps/web-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web-server/tsconfig.app.json"
      }
    },
    "admin-server": {
      "type": "application",
      "root": "apps/admin-server",
      "entryFile": "main",
      "sourceRoot": "apps/admin-server/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-server/tsconfig.app.json"
      }
    }
  }
}